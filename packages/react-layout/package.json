{
  "name": "@workspace/react-layout",
  "type": "module",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "sideEffects": false,
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md",
    "tailwind.config.ts",
    "src/styles.css"
  ],
  "imports": {
    "#*": [
      "./dist/*",
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx"
    ]
  },
  "exports": {
    "./package.json": "./package.json",
    "./errors": [
      "./src/components/errors/*.tsx"
    ],
    "./landing": [
      "./src/components/landing/*.tsx"
    ],
    "./components/*": [
      "./src/components/*.tsx"
    ],
    "./*.layout": [
      "./src/*.layout.tsx"
    ],
    ".": "./src/index.ts"
  },
  "devDependencies": {
    "@mantine/hooks": "^7.13.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.8.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@workspace/data": "workspace:*",
    "@workspace/react-ui": "workspace:*",
    "@workspace/tw-preset-workspace": "workspace:*",
    "@workspace/tw-style": "workspace:*",
    "autoprefixer": "^10.4.20",
    "fluid-tailwind": "^1.0.3",
    "glob": "^11.0.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "postcss-discard-comments": "^7.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-router-dom": "^6.27.0",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "@mantine/hooks": "^7.13.4",
    "@workspace/data": "workspace:*",
    "@workspace/react-ui": "workspace:*",
    "@workspace/tw-preset-workspace": "workspace:*",
    "@workspace/tw-style": "workspace:*",
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0",
    "react-error-boundary": "^4.1.2",
    "react-router-dom": "^6.27.0"
  },
  "dependencies": {
    "@workspace/data": "workspace:*",
    "@workspace/react-ui": "workspace:*"
  }
}
